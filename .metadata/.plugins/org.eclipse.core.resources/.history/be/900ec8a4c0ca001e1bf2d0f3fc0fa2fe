package round_2;

public class Set1 {
	
	public static void printPattern(int n) {
		int odd = 1;
		int even = 2;
		
		for(int i=1; i<=n; i++) {
			int spaces = n-i;
			for(int j=1; j<=n; j++) {
				if(j <= spaces) {
					System.out.print("  ");
					continue;
				}
				if(i % 2 == 1) {
					if(odd < 10) System.out.print(odd + "   ");
					else System.out.print(odd + " ");
					odd += 2;
				}else {
					if(even < 10) System.out.print(even + "   ");
					else System.out.print(even + " ");
					even += 2;	
				}
			}
			System.out.println();
		}
	}

	public static void main(String[] args) {
		printPattern(5);

	}

}
