package synchronized_;

import java.util.Iterator;

public class SchoolThread implements Runnable{
	
	private School school;
	
	SchoolThread(School school) {
		this.school = school;	
	}

	@Override
	public void run() {
		for(int i=0; i<10; i++) {
			System.out.println(Thread.currentThread().getName() + " " + school.getName() + " " + i);
		}
		
		
	}

}
